<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" vgap="15.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <PasswordField fx:id="tabNr" onKeyReleased="#textFieldDisable" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Label text="User ID" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <TextField fx:id="harnessOne" onKeyReleased="#onHarnessOneEntered" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
    <Label text="Packing Label Code 1" GridPane.rowIndex="5"/>
    <TextField fx:id="harnessTwo" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    <Label text="Testing Label Code" GridPane.rowIndex="3"/>

    <Button fx:id="oK" onAction="#onButtonClicked" prefHeight="10" prefWidth="300" text="OK" GridPane.columnIndex="1"
            GridPane.rowIndex="10"/>
    <Button fx:id="reset" onAction="#reset" prefHeight="10" prefWidth="100" text="Restart" GridPane.columnIndex="1"
            GridPane.halignment="RIGHT" GridPane.rowIndex="0"/>
    <TextField fx:id="ats" prefHeight="30" prefWidth="300" GridPane.columnIndex="1" GridPane.rowIndex="12"
               GridPane.rowSpan="5"/>
    <TextField fx:id="tabDisplay" prefHeight="30" prefWidth="200" GridPane.rowIndex="12" GridPane.rowSpan="5"/>
    <Label prefHeight="46.0" prefWidth="150.0" text="User ID" GridPane.rowIndex="12" GridPane.rowSpan="2"/>
    <Label fx:id="myDate" text="Date" GridPane.rowIndex="9"/>
    <Label fx:id="myTime" text="Time" GridPane.rowIndex="10"/>
    <TextField fx:id="harnessThree" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
    <TextField fx:id="harnessFour" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
    <Label text="Packing Label Code 2" GridPane.rowIndex="6"/>
    <Label text="Packing Label Code 3" GridPane.rowIndex="7"/>
    <Label fx:id="lblTimer" alignment="CENTER" prefHeight="50.0" prefWidth="143.0" text="Label" textAlignment="CENTER"
           GridPane.columnIndex="1"/>
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
    </rowConstraints>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</GridPane>
